package org.lifeform.application;

public class LifeformAppException extends Exception {
	private static final long serialVersionUID = 2443094256528742437L;

	public LifeformAppException() {
		super();
	}

	public LifeformAppException(final String message, final Throwable cause) {
		super(message, cause);
	}

	public LifeformAppException(final String message) {
		super(message);
	}

	public LifeformAppException(final Throwable cause) {
		super(cause);
	}

	public LifeformAppException(final String string, final Exception ex) {
		super(string, ex);
	}

}
