package org.lifeform.math;

public final class LogBase10Function implements InvertibleFunction {

	public final double evaluate(final double x) {
		return Math.log(x) / Util.LOG10;
	}

	public final double getInverse(final double y) {
		return Math.pow(10, y);
	}
}

