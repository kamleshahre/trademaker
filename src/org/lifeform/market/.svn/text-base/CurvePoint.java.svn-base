package org.lifeform.market;

import java.util.Calendar;

import org.lifeform.core.Pair;

public class CurvePoint extends Pair<Calendar, Double> {

	public CurvePoint(final Calendar first, final Double second) {
		super(first, second);
	}

	public Calendar getDate() {
		return getFirst();
	}

	public Double getValue() {
		return getSecond();
	}
}
