package org.lifeform.product;

public class Bond extends Product {

	enum Type {
		Treasury, Corporate, Bund, OAT, Canada, Japan, Italy, France
	}

	private double faceValue = 1000;

	public Bond(final int id) {
		super(id);
	}

	public double getFaceValue() {
		return faceValue;
	}

	public void setFaceValue(final double faceValue) {
		this.faceValue = faceValue;
	}
}
